    "axios": "^0.21.1",
    "date-fns": "^2.14.0",
    "deepmerge": "^4.2.2",
    "gatsby": "^2.31.1",
    "gatsby-plugin-algolia": "^0.16.2",
    "gatsby-plugin-catch-links": "^2.9.0",
    "gatsby-plugin-feed": "^2.12.0",
    "gatsby-plugin-google-analytics": "^2.10.0",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-mdx": "^1.9.0",
    "gatsby-plugin-netlify": "^2.10.0",
    "gatsby-plugin-offline": "^3.9.0",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sitemap": "^2.11.0",
    "gatsby-plugin-typescript": "^2.11.0",
    "gatsby-source-filesystem": "^2.10.0",
    "prism-react-renderer": "^1.0.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.8.3",
    "react-redux": "^7.2.2",
    "react-scroll": "^1.7.16",
    "redux": "^4.0.5"
