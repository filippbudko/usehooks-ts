    "plop": "^2.7.1",
    "prettier": "^2.0.4",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Junscuzzy/usehooks.ts"
