    "axios": "^0.19.2",
    "date-fns": "^2.14.0",
    "deepmerge": "^4.2.2",
    "gatsby": "^2.24.8",
    "gatsby-plugin-algolia": "^0.8.1",
    "gatsby-plugin-catch-links": "^2.2.3",
    "gatsby-plugin-feed": "^2.4.4",
    "gatsby-plugin-google-analytics": "^2.2.4",
    "gatsby-plugin-manifest": "^2.3.6",
    "gatsby-plugin-material-ui": "^2.1.6",
    "gatsby-plugin-mdx": "^1.1.9",
    "gatsby-plugin-netlify": "^2.3.11",
    "gatsby-plugin-offline": "^3.1.4",
    "gatsby-plugin-react-helmet": "^3.2.4",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sitemap": "^2.3.6",
    "gatsby-plugin-typescript": "^2.3.3",
