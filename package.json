    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "jest": "^26.6.3",
    "jest-preset-gatsby": "^1.0.108",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.1",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
