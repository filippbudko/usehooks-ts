    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/mdx-js__react": "^1.5.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-instantsearch-dom": "^6.3.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-scroll": "^1.5.4",
    "@types/redux": "^3.6.0",
