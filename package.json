{
  "name": "useHooks.ts",
  "private": true,
  "description": "Simple React hooks ready to use written in typescript.",
  "version": "0.1.0",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "eslint",
    "prettier",
    "material-ui"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run copy:hooks && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "npm run copy:hooks && gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "run-p lint:**",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:typescript": "tsc",
    "format:content": "prettier --write --loglevel silent \"src/content/**/*.{md,mdx}\"",
    "copy:hooks": "node ./tools/copyHooks.js && npm run format:content",
    "plop": "plop && npm run lint:prettier",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
