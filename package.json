{
  "name": "useHooks.ts",
  "private": true,
  "description": "Simple React hooks ready to use written in typescript.",
  "version": "0.1.0",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "eslint",
    "prettier",
    "material-ui"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run copy:hooks",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "develop": "npm run copy:hooks && gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "lint-fix": "npm run lint:eslint -- --fix && npm run lint:prettier",
    "lint": "run-p lint:**",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc",
    "upgrade-interactive": "npm-check --update",
    "format:content": "prettier --write --loglevel silent \"./**/*.{md,mdx}\"",
    "copy:hooks": "node ./tools/copyHooks.js && npm run format:content && npm run update:readme",
    "update:readme": "node ./tools/updateReadme.js",
    "generate": "plop && npm run update:readme && npm run lint:prettier",
    "pretest": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" ",
    "test": "npm run lint:eslint && npm run typecheck && jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.4",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "axios": "^0.21.1",
    "date-fns": "^2.21.3",
    "deepmerge": "^4.2.2",
    "gatsby": "^3.5.1",
    "gatsby-plugin-algolia": "^0.16.3",
    "gatsby-plugin-catch-links": "^3.5.0",
    "gatsby-plugin-feed": "^3.5.0",
    "gatsby-plugin-google-analytics": "^3.5.0",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-mdx": "^2.5.1",
    "gatsby-plugin-netlify": "^3.5.0",
    "gatsby-plugin-offline": "^4.5.1",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-robots-txt": "^1.6.2",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sitemap": "^4.1.0",
    "gatsby-plugin-typescript": "^3.5.0",
    "gatsby-source-filesystem": "^3.5.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.11.0",
    "react-scroll": "^1.8.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.1",
    "@types/axios": "^0.14.0",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.23",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-helmet": "^6.1.1",
    "@types/react-instantsearch-dom": "^6.10.0",
    "@types/react-scroll": "^1.8.2",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.1",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gatsby-cli": "^3.5.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "jest": "^26.6.3",
    "jest-preset-gatsby": "^1.0.125",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "plop": "^2.7.1",
    "postcss": "^8.3.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliencrn/usehooks.ts"
  },
  "bugs": {
    "url": "https://github.com/juliencrn/usehooks.ts/issues"
  }
}
