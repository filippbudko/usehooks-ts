{
  "name": "usehooks.ts",
  "description": "React hook library, ready to use, written in Typescript.",
  "version": "0.1.1",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "keywords": [
    "typescript",
    "react-hooks",
    "npm"
  ],
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run types-check",
    "clean": "rimraf -rf ./dist",
    "types-check": "tsc -p tsconfig.build.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "npm run lint -- --fix && npm run format",
    "format": "prettier --write --loglevel silent \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "prepublishOnly": "npm test && npm run build",
    "preversion": "npm test",
    "prepare-old": "npm run build",
    "publish": "npm publish",
    "update": "npm update",
    "pretest": "npm run lint && npm run types-check",
    "test": "jest --config ../../jest.config.json --rootDir .",
    "test:watch": "npm run test -- --watch",
    "upgrade-interactive": "npm-check --update"
  },
  "devDependencies": {
    "@junscuzzy/eslint-config": "^1.0.2",
    "@junscuzzy/prettier-config": "^1.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/node": "^16.9.1",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^27.2.0",
    "jest-preset-gatsby": "^1.0.134",
    "npm-check": "^5.9.2",
    "prettier": "^2.4.0",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliencrn/usehooks.ts"
  },
  "bugs": {
    "url": "https://github.com/juliencrn/usehooks.ts/issues"
  },
  "dependencies": {
    "react": "^17.0.2"
  }
}
