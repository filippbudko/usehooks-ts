{
  "name": "usehooks-ts",
  "description": "React hook library, ready to use, written in Typescript.",
  "version": "0.0.1",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "keywords": [
    "typescript",
    "react-hooks",
    "npm"
  ],
  "license": "MIT",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf -rf ./dist",
    "types-check": "tsc -p tsconfig.json",
    "compile": "tsc -p tsconfig.build.json --rootDir ./src",
    "prepublishOnly": "npm run build",
    "preversion": "npm test",
    "prepare": "npm run build",
    "publish": "npm publish",
    "update": "npm update",
    "upgrade-interactive": "npm-check --update"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "npm-check": "^5.9.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliencrn/usehooks.ts"
  },
  "bugs": {
    "url": "https://github.com/juliencrn/usehooks.ts/issues"
  }
}
