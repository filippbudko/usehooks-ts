{
  "name": "useHooks.ts",
  "private": true,
  "description": "React hook library, ready to use, written in Typescript.",
  "version": "0.1.0",
  "author": "Julien CARON <juliencaron@protonmail.com>",
  "keywords": [
    "gatsby",
    "typescript",
    "eslint",
    "prettier",
    "material-ui",
    "jest",
    "@testing-library",
    "prism",
    "mdx"
  ],
  "license": "MIT",
  "scripts": {
    "old-prebuild": "npm run copy:hooks && npm run test",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "serve": "gatsby serve",
    "old-develop": "npm run copy:hooks && npm run test && gatsby develop",
    "develop": "npm run test && gatsby develop",
    "start": "npm run develop",
    "clean": "gatsby clean",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint-fix": "npm run lint -- --fix && npm run format",
    "format": "prettier --write --loglevel silent \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check": "tsc",
    "copy:hooks": "node ./tools/copyHooks.js && npm run update:readme && npm run format",
    "update:readme": "node ./tools/updateReadme.js",
    "plop": "plop && npm run update:readme && npm run format",
    "pretest": "npm run check && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade-interactive": "npm-check --update"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "date-fns": "^2.23.0",
    "deepmerge": "^4.2.2",
    "gatsby": "^3.10.2",
    "gatsby-plugin-algolia": "^0.22.0",
    "gatsby-plugin-catch-links": "^3.10.0",
    "gatsby-plugin-feed": "^3.10.0",
    "gatsby-plugin-google-analytics": "^3.10.0",
    "gatsby-plugin-manifest": "^3.10.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-mdx": "^2.10.1",
    "gatsby-plugin-netlify": "^3.10.0",
    "gatsby-plugin-offline": "^4.10.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-robots-txt": "^1.6.8",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-sitemap": "^4.6.0",
    "gatsby-plugin-typescript": "^3.10.0",
    "gatsby-source-filesystem": "^3.10.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0",
    "react-instantsearch-dom": "^6.12.0",
    "react-scroll": "^1.8.3"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^26.0.24",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.4.7",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/react-instantsearch-dom": "^6.12.0",
    "@types/react-scroll": "^1.8.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.1",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "gatsby-cli": "^3.10.0",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "jest": "^27.0.6",
    "jest-preset-gatsby": "^1.0.125",
    "npm-check": "^5.9.2",
    "plop": "^2.7.1",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliencrn/usehooks.ts"
  },
  "bugs": {
    "url": "https://github.com/juliencrn/usehooks.ts/issues"
  }
}
